{
  "name": "swos-cli",
  "version": "1.0.0",
  "description": "CLI to manage Mikrotik SwOS devices",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "swos-cli": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
