    "lint": "@biomejs/biome check .",
    "format": "@biomejs/biome format --write ."
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.0",
    "@types/node": "^20.0.0",
    "msw": "^2.12.7",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0",
    "undici": "^7.18.2",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "commander": "^12.0.0",
    "digest-fetch": "^1.3.0",
    "dotenv": "^16.4.0",
    "node-fetch": "^2.7.0",
    "zod": "^3.22.0"
  }
}
